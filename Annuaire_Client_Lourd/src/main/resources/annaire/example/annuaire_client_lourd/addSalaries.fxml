<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="annaire.example.annuaire_client_lourd.AddSalariesController">
    <children>
        <AnchorPane prefHeight="65.0" prefWidth="600.0" style="-fx-background-color: #0080FF;">
            <children>
                <Text layoutX="216.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ajouter un salarié">
                    <font>
                        <Font name="System Bold" size="19.0" />
                    </font>
                </Text>
            </children>
        </AnchorPane>
        <TextField fx:id="email" layoutX="109.0" layoutY="222.0" prefHeight="26.0" prefWidth="350.0" promptText="Email" />
        <Button fx:id="buttonCancelAddSalaries" layoutX="173.0" layoutY="342.0" mnemonicParsing="false" onAction="#cancelAndReturnToSalariesPage" prefHeight="26.0" prefWidth="80.0" style="-fx-background-color: red;" text="Annuler">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
        <Button fx:id="buttonValidateAddSalaries" layoutX="333.0" layoutY="342.0" mnemonicParsing="false" onAction="#addAndReturnToSalariesPage" prefHeight="26.0" prefWidth="80.0" style="-fx-background-color: #32CD32;" text="Confirmer">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
        <TextField fx:id="prenom" layoutX="109.0" layoutY="102.0" prefHeight="26.0" prefWidth="150.0" promptText="Prénom" />
        <TextField fx:id="nom" layoutX="310.0" layoutY="102.0" prefHeight="26.0" prefWidth="150.0" promptText="Nom" />
        <TextField fx:id="fixe" layoutX="109.0" layoutY="162.0" prefHeight="26.0" prefWidth="150.0" promptText="Fixe" />
        <TextField fx:id="portable" layoutX="310.0" layoutY="162.0" prefHeight="26.0" prefWidth="150.0" promptText="Portable" />
        <TextField fx:id="id_site" layoutX="157.0" layoutY="282.0" prefHeight="26.0" prefWidth="112.0" promptText="Id site" />
        <TextField fx:id="id_service" layoutX="317.0" layoutY="282.0" prefHeight="26.0" prefWidth="112.0" promptText="Id service" />
    </children>
</AnchorPane>
